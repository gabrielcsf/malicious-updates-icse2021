{
  "source": "./_posts",
  "destination": "./_site",
  "metadata": {
    "title": "My Jekyll-Powered Blog",
    "description": "My second, super-cool, Jekyll-powered blog."
  },
  "plugins": {
    "metalsmith-drafts": {},
    "metalsmith-markdown": {},
    "metalsmith-permalinks": {
      "pattern": ":title"
    },
    "metalsmith-layouts": {
      "engine": "swig",
      "directory": "_layouts"
    }
  }
}
